{
	"name": "demo",
	"description": "Demo for Laravel Framework.",
	"keywords": [],
	"license": "GPL 2.0",
	"type": "project",
	"require": {
      "guzzlehttp/guzzle": "~6.0",
      "zircote/swagger-php": "^2.0"
	},
	"require-dev": {

	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/",
			"ArrayTree\\": "libraries/ArrayTree/ArrayTree/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
